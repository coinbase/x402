{
  "name": "@b3dotfun/anyspend-x402-hono",
  "version": "0.1.1",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "tsx --env-file=.env index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "tsup",
    "watch": "tsc --watch",
    "format": "prettier -c .prettierrc --write \"**/*.{ts,js,cjs,json,md}\"",
    "format:check": "prettier -c .prettierrc --check \"**/*.{ts,js,cjs,json,md}\"",
    "lint": "eslint . --ext .ts --fix",
    "lint:check": "eslint . --ext .ts"
  },
  "keywords": [
    "x402",
    "anyspend",
    "hono",
    "middleware",
    "payment-protocol",
    "web3",
    "payments",
    "cryptocurrency"
  ],
  "license": "Apache-2.0",
  "author": "b3-fun",
  "repository": {
    "type": "git",
    "url": "https://github.com/b3-fun/anyspend-x402.git"
  },
  "description": "AnySpend x402 Hono middleware - Extended x402 payment protocol integration for Hono with multi-token support",
  "homepage": "https://github.com/b3-fun/anyspend-x402#readme",
  "bugs": {
    "url": "https://github.com/b3-fun/anyspend-x402/issues"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/node": "^22.13.4",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^9.24.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.9",
    "eslint-plugin-prettier": "^5.2.6",
    "prettier": "3.5.2",
    "tsup": "^8.4.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vite": "^6.2.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.5"
  },
  "dependencies": {
    "@coinbase/cdp-sdk": "^1.29.0",
    "@solana/kit": "^2.1.1",
    "hono": "^4.7.1",
    "viem": "^2.21.26",
    "@b3dotfun/anyspend-x402": "workspace:^",
    "@b3dotfun/anyspend-x402-token-compat": "workspace:^",
    "zod": "^3.24.2"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.mts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  }
}
