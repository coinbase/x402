{
  "name": "@b3dotfun/anyspend-x402-token-compat",
  "version": "0.1.1",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "tsx --env-file=.env index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "tsup",
    "watch": "tsc --watch",
    "format": "prettier -c .prettierrc --write \"**/*.{ts,js,cjs,json,md}\"",
    "format:check": "prettier -c .prettierrc --check \"**/*.{ts,js,cjs,json,md}\"",
    "lint": "eslint . --ext .ts --fix",
    "lint:check": "eslint . --ext .ts"
  },
  "keywords": [
    "x402",
    "anyspend",
    "token",
    "eip-2612",
    "eip-3009",
    "permit",
    "compatibility",
    "web3",
    "payments",
    "cryptocurrency"
  ],
  "license": "Apache-2.0",
  "author": "b3-fun",
  "repository": {
    "type": "git",
    "url": "https://github.com/b3-fun/anyspend-x402.git"
  },
  "description": "Token compatibility checker for EIP-2612 (Permit) and EIP-3009 (TransferWithAuthorization) support",
  "homepage": "https://github.com/b3-fun/anyspend-x402#readme",
  "bugs": {
    "url": "https://github.com/b3-fun/anyspend-x402/issues"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/node": "^22.13.4",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^9.24.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.9",
    "eslint-plugin-prettier": "^5.2.6",
    "prettier": "3.5.2",
    "tsup": "^8.4.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.5",
    "vite": "^6.2.6"
  },
  "dependencies": {
    "zod": "^3.24.2"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.mts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  }
}
