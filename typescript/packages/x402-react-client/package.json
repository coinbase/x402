  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "watch": "tsc --watch",
    "format": "prettier -c .prettierrc --write \"**/*.{ts,tsx,js,cjs,json,md}\"",
    "format:check": "prettier -c .prettierrc --check \"**/*.{ts,tsx,js,cjs,json,md}\"",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx"
  },
  "keywords": [
    "x402"
  ],
  "license": "Apache-2.0",
  "author": "John O. (Johnex)",
  "repository": "https://github.com/coinbase/x402",
  "description": "React hooks and components for x402 Payment Protocol",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "viem": "^2.38.6",
    "wagmi": "2.19.2"
  },
  "dependencies": {
    "@rainbow-me/rainbowkit": "^2.2.9",
    "@tanstack/react-query": "^5.90.7",
    "x402": "workspace:^"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "esbuild-plugin-preserve-directives": "^0.0.11",
    "esbuild-plugin-react18": "^0.2.6",
    "eslint": "^9.39.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^61.1.12",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "prettier": "3.6.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "viem": "^2.38.6",
    "wagmi": "^2.19.2"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
