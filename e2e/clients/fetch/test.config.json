    "eip2612GasSponsoring"
  ],
  "environment": {
    "required": [
      "EVM_PRIVATE_KEY",
      "SVM_PRIVATE_KEY",
      "RESOURCE_SERVER_URL",
      "ENDPOINT_PATH"
    ],
    "optional": []
  }
}