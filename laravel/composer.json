{
    "name": "coinbase/x402-laravel",
    "description": "Laravel integration for the Coinbase x402 PHP SDK",
    "type": "library",
    "require": {
      "php": "^8.2",
      "orchestra/testbench": "^8.0"
    },
    "autoload": {
      "psr-4": {
        "Coinbase\\X402\\Laravel\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Coinbase\\X402\\Laravel\\Tests\\": "src/tests/"
      }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
  }